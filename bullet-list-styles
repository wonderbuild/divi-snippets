.et_pb_text ul li {
  list-style: none; 
  text-indent:-15px;
  padding-left:15px;
}
.et_pb_text ul li::before {
  color: #da8e11;  /* color of bullet or square */
  content: "\2022"; /* Unicode of character to precede the list item */
  font-size: 2em; /* em or %*/
  padding-right: .2em; /* use em*/
  position: relative;
  top: 0em; /* use em or % */ 
}
